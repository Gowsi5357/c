include<stdio.h> 
int main()
{
int i,j,l,count=0;
printf("Enter the number of rows/columns\n");
scanf("%d",&l);
for(int i=0;i<l;i++)
{ for(j=0;j<l-1;j++)
{ count++; 
} 
}
for(i=0;i<l;i++) {
for(j=0;j<l;j++)
{ count++; 
if(j==l-1) 
{ printf("%d",count);
} 
else 
{ printf("%d*",count);
}
} 
count=count-2*l; 
printf("\n");
}
}
